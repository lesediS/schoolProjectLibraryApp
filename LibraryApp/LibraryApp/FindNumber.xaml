<Window x:Class="LibraryApp.FindNumber"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:LibraryApp"
        mc:Ignorable="d"
        Title="FindNumber" Height="450" Width="800">
    <Canvas>
        <TextBlock x:Name="CallNumber" Width="100" Height="30" Canvas.Left="10" Canvas.Top="10" Text="0" />
        
        <TextBlock x:Name="Score" Width="100" Height="30" Canvas.Left="10" Canvas.Top="90" />
        
            <TreeView Name="DeweyTreeView" Width="300" Height="300" Canvas.Right="10" Canvas.Top="50" SelectedItemChanged="DeweyTreeView_SelectedItemChanged"></TreeView>
        
    </Canvas>
</Window>
